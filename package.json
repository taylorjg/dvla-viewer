{
  "name": "dvla-viewer",
  "description": "Simple app to lookup car registrations using DVLA API",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx,cjs --report-unused-disable-directives --max-warnings 0",
